# defines a recursive fibonacci sequence
function fib(n) {
	if (n < 2) return 1;
	return fib(n-2) + fib(n-1);
}

local n = (vargv.len() != 0) ? vargv[0].tointeger() : 1;
print(fib(b) + "\n")

// Integer
local a = 123;
local b = 0x0012;
local c = 075;
local d = 'w';

// Floating points
local a = 1.0;
local b = 0.234;
local c = 1.52;
local d = 1.e2;
local e = 1.e-2;
local f = 1.0e-2;
